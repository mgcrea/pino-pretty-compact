{
  "name": "@mgcrea/pino-pretty-compact",
  "version": "0.5.0",
  "description": "Compact request logger plugin for fastify written in TypeScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Olivier Louvignes <olivier@mgcrea.io>",
  "repository": "github:mgcrea/pino-pretty-compact",
  "license": "MIT",
  "access": "public",
  "scripts": {
    "build": "rm -rf lib/*; tsc --build",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prettycheck": "prettier --check 'src/' --check 'test/'",
    "lint": "eslint src/ test/",
    "prepare": "npm run build"
  },
  "keywords": [
    "pino",
    "pretty",
    "compact",
    "fastify",
    "logger"
  ],
  "devDependencies": {
    "@mgcrea/fastify-request-logger": "^0.5.2",
    "@tsconfig/node10": "^1.0.7",
    "@types/http-errors": "^1.8.0",
    "@types/ioredis": "^4.26.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/pino": "^6.3.8",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "fastify": "^3.17.0",
    "http-errors": "^1.8.0",
    "jest": "^27.0.3",
    "pino-pretty": "^5.0.1",
    "prettier": "^2.3.0",
    "ts-jest": "^27.0.2",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "chalk": "^4.1.1"
  }
}
