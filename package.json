{
  "name": "@mgcrea/pino-pretty-compact",
  "version": "0.6.0",
  "description": "Compact request logger plugin for fastify written in TypeScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Olivier Louvignes <olivier@mgcrea.io>",
  "repository": "github:mgcrea/pino-pretty-compact",
  "license": "MIT",
  "access": "public",
  "scripts": {
    "start": "npm run spec -- --watch",
    "build": "rimraf lib/*; tsc --build",
    "lint": "eslint src/ test/",
    "prettycheck": "prettier --check src/ test/",
    "typecheck": "tsc --noEmit",
    "spec": "DEBUG=fastify-session*,prisma-query jest --runInBand",
    "test": "npm run lint && npm run prettycheck && npm run typecheck && npm run spec",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "pino",
    "pretty",
    "compact",
    "fastify",
    "logger"
  ],
  "devDependencies": {
    "@mgcrea/fastify-request-logger": "^0.6.1",
    "@tsconfig/node14": "^1.0.3",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^28.1.4",
    "@types/node": "^16.11.42",
    "@types/pino": "^7.0.4",
    "@types/pino-pretty": "^4.7.5",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "fastify": "^4.2.0",
    "http-errors": "^2.0.0",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "pino-pretty": "^8.1.0"
  }
}
